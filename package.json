{
    "name": "keepix.server",
    "version": "0.0.164",
    "description": "",
    "author": "",
    "private": true,
    "license": "MIT",
    "scripts": {
        "copy-bin": "copyfiles -u 1 bin/keepix-server dist/bin",
        "copy-package": "copyfiles -u 1 bin/package.json dist/",
        "install-latest-front": "npm install keepix-application-interface-build@latest",
        "increase-build-version": "increase-build-package-public-directory && increase-build-package-public-directory bin",
        "build": "npm run install-latest-front && nest build && npm run increase-build-version && npm run copy-bin && npm run copy-package",
        "publish": "cd dist && npm publish",
        "start": "nest start -- --disable-front",
        "start-with-front": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main"
    },
    "dependencies": {
        "@ajimae/retry": "^0.0.1",
        "@cosmjs/proto-signing": "^0.32.0",
        "@cosmjs/stargate": "^0.32.0",
        "@ethersproject/hdnode": "^5.7.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/swagger": "^7.1.11",
        "@silentbot1/nat-api": "^0.4.7",
        "bitcore-lib": "^10.0.17",
        "check-disk-space": "^3.4.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "decompress": "^4.2.1",
        "decompress-targz": "^4.1.1",
        "ethers": "^5.1.4",
        "express": "^4.18.2",
        "http": "^0.0.1-security",
        "https": "^1.0.0",
        "increase-build-package-public-directory": "^0.0.3",
        "keepix-application-interface-build": "^0.0.32",
        "keepix-ssh": "^0.4.22",
        "keepix-tokens": "^0.0.11",
        "moment": "^2.29.4",
        "nocache": "^4.0.0",
        "node-fetch": "^2.7.0",
        "node-schedule": "^2.1.1",
        "nodejs-file-downloader": "^4.12.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sotez": "^13.0.1",
        "wireless-tools": "^0.19.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "copyfiles": "^2.4.1",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}